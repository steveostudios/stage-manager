<form role="form" style="margin:0 auto;width:400px;">
	<div class="form-group">
    <label for="inp_email">Email</label>
    <input type="text" class="form-control" id="inp_email" placeholder="Email Address" value="<%= username %>">
  </div>
  <div class="form-group">
    <label for="inp_fname">First Name</label>
    <input type="text" class="form-control" id="inp_fname" placeholder="First Name" value="<%= fname %>">
  </div>
  <div class="form-group">
    <label for="inp_lname">Last Name</label>
    <input type="text" class="form-control" id="inp_lname" placeholder="Last Name" value="<%= lname %>">
  </div>
  <button type="submit" class="btn btn-default">Update</button>
</form>

<div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
    <span class="sr-only">60% Complete</span>
  </div>
</div>